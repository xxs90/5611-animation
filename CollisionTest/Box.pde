public class Box {
  int id;
  float centerX;
  float centerY;
  float widthBox;
  float heightBox;

  public Box(int id, float centerX, float centerY, float widthBox, float heightBox) {
    this.id = id;
    this.centerX = centerX;
    this.centerY = centerY;
    this.widthBox = widthBox;
    this.heightBox = heightBox;
  }
}
